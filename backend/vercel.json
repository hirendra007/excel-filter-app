{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",   
      "dest": "/server.js"
    },
    {
      "src": "/favicon.ico",      
      "headers": {
        "Cache-Control": "max-age=86400"
      },
      "status": 204
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
